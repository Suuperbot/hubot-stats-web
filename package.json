    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-react": "^6.7.1",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
